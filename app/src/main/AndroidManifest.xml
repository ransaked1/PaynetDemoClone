<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.paynetdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.AccountActivities.EditActivity"
            android:label="@string/title_activity_edit"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".ui.AccountActivities.IdentificationActivity"
            android:label="@string/title_activity_identification"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.AccountActivities.SettingsActivity"
            android:label="@string/title_activity_setting"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.AccountActivities.SupportActivity"
            android:label="@string/title_activity_support"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.AccountActivities.LoyaltyActivity"
            android:label="@string/title_activity_loyalty"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.AccountActivities.OnboardingActivity"
            android:label="@string/title_activity_onboarding"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.AccountActivities.FaqActivity"
            android:label="@string/title_activity_faq"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.AccountActivities.TermsCondActivity"
            android:label="@string/title_activity_terms_cond"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CardActivity" />
        <activity android:name=".ui.WalletActivities.History" android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.WalletActivities.Discount_card" />
        <activity android:name=".ui.WalletActivities.Withdraw" />
        <activity android:name=".ui.WalletActivities.Topup" />
        <activity android:name=".ui.WalletActivities.Transfer" />
        <activity android:name=".ui.WalletActivities.Pay" />
        <activity android:name=".ui.WalletActivities.Mybills" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>